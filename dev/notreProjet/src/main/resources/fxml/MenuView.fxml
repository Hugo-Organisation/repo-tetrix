<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="650.0" spacing="100.0" styleClass="root" stylesheets="@../css/MenuViewCSS.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.MenuViewCtrl">
   <children>
      <Text fx:id="Title" strokeType="OUTSIDE" strokeWidth="0.0" text="SANDTRIS" textAlignment="CENTER">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Text>
      <HBox alignment="CENTER" spacing="60.0">
         <children>
            <Button fx:id="aboutButton" mnemonicParsing="false" onMouseClicked="#onClic" text="ABOUT" />
            <Button fx:id="startButton" mnemonicParsing="false" onMouseClicked="#onClic" text="START" />
            <Button fx:id="leaderboardButton" mnemonicParsing="false" onMouseClicked="#onClic" text="LEADERBOARD" />
         </children>
      </HBox>
      <VBox alignment="CENTER">
         <children>
            <Text fx:id="MusicTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="MUSIC" />
            <Slider fx:id="MusicSlider" max="20.0" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" />
         </children>
      </VBox>
      <HBox alignment="CENTER" spacing="60.0">
         <children>
            <Button fx:id="parametresButton" mnemonicParsing="false" onMouseClicked="#onClic" text="PARAMETRES" />
            <Button fx:id="exitButton" mnemonicParsing="false" onMouseClicked="#onClic" text="EXIT" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
